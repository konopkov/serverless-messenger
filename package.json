{
  "name": "serverless-messenger",
  "version": "0.1.0",
  "bin": {
    "serverless-messenger": "bin/serverless-messenger.js"
  },
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "build": "tsc",
    "watch": "./build.js -watch -sourcemap",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "cdk": "cdk",
    "cdk-local": "cdk synth --no-staging > local/template.yaml",
    "cdk-local:debug": "DEBUG=true npm run cdk-local",
    "invoke": "sam local invoke serverlessmessengergraphqlhandlerdev57BD572B  -t local/template.yaml --env-vars local/environment.json",
    "invoke:debug": "npm run invoke -- --debug-port 5858"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@types/aws-lambda": "^8.10.101",
    "@types/jest": "^27.5.0",
    "@types/joi-phone-number": "^5.0.3",
    "@types/node": "^10.17.27",
    "@types/nunjucks": "^3.2.1",
    "@types/prettier": "2.6.0",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "aws-cdk": "2.27.0",
    "esbuild": "^0.14.49",
    "eslint": "^8.20.0",
    "jest": "^27.5.1",
    "jest-raw-loader": "^1.0.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.131.0",
    "@aws-sdk/client-ses": "^3.131.0",
    "@aws-sdk/client-sns": "^3.131.0",
    "aws-cdk-lib": "2.27.0",
    "constructs": "^10.0.0",
    "inversify": "^6.0.1",
    "joi": "^17.6.0",
    "joi-phone-number": "^5.1.1",
    "nunjucks": "^3.2.3",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "ulid": "^2.3.0"
  }
}
